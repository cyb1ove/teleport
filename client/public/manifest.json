{
  "manifest_version": 3,
  "name": "Teleport",
  "version": "0.0.1",
  "description": "Browser extension for exporting telegram messages",
  "permissions": [
    "activeTab",
    "tabs"
  ],
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "icons": {
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "background": {
    "service_worker": "background.js",
    "type": "module"
  }
}
